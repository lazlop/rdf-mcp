{
  "$schema": "https://gofastmcp.com/public/schemas/fastmcp.json/v1.json",
  "source": {
    "type": "filesystem",
    "path": "kgqa.py",
    "entrypoint": "mcp"
  },
  "environment": {
    "type": "uv",
    "python": ">=3.10",
    "dependencies": [
      "mcp[cli]",
      "rdflib",
      "oxrdflib"
    ]
  },
  "deployment": {
    "transport": "stdio",
    "log_level": "INFO",
    "env": {
      "GRAPH_FILE": "test-building.ttl"
    }
  }
}